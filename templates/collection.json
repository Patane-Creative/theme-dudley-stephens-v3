{
  "sections": {
    "collection-header": {
      "type": "collection-header",
      "custom_css": [
        "h1 {text-align: center;}"
      ],
      "settings": {
        "enable": false,
        "collection_image_enable": false,
        "parallax": false,
        "parallax_direction": "top"
      }
    },
    "main-collection": {
      "type": "main-collection",
      "blocks": {
        "product_grid": {
          "type": "product_grid",
          "settings": {
            "enable_collection_count": true,
            "per_row": 3,
            "rows_per_page": 7,
            "mobile_flush_grid": true
          }
        }
      },
      "block_order": [
        "product_grid"
      ],
      "custom_css": [
        "span.h6 {color: var(--colorPrice);}",
        ".grid-product__title {font-weight: 500;}"
      ],
      "settings": {
        "enable_sidebar": true,
        "collapsed": true,
        "filter_style": "sidebar",
        "enable_color_swatches": false,
        "enable_sort": true
      }
    }
  },
  "order": [
    "collection-header",
    "main-collection"
  ]
}
