{
   "name": "theme-dudley-stephens-v2",
   "version": "2.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "fst:build": "vue-cli-service build --dest ./assets --formats umd,umd-min --target lib --inline-vue --name freeShippingThreshold _src/vue/free-shipping-threshold/free-shipping-threshold.js --no-clean && rm _src/js/demo.html",
      "gwp:build": "vue-cli-service build --dest ./assets --formats umd,umd-min --target lib --inline-vue --name giftWithPurchase _src/vue/gift-with-purchase/gift-with-purchase.js --no-clean && rm _src/js/demo.html",
      "vue:build": "npm run fst:build && npm run gwp:build",
      "js:checkout:dev": "webpack --env development",
      "js:checkout:prod": "webpack --env production",
      "js:theme:dev": "watch 'uglifyjs ./assets/theme.js --output ./assets/theme.min.js' ./assets/",
      "js:theme:prod": "uglifyjs ./assets/theme.js --output ./assets/theme.min.js",
      "js:fitguide": "parcel _src/js/fitguide.js --dist-dir assets",
      "shop:start": "shopify theme dev --store=dudley-stephens",
      "shop:start:dev": "shopify theme dev --store=dudleystephens-dev"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/lucidnz/theme-dudley-stephens-v2.git"
   },
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/lucidnz/theme-dudley-stephens-v2/issues"
   },
   "homepage": "https://github.com/lucidnz/theme-dudley-stephens-v2#readme",
   "devDependencies": {
      "@babel/core": "^7.12.16",
      "@babel/eslint-parser": "^7.12.16",
      "@vue/cli-plugin-babel": "~5.0.0",
      "@vue/cli-plugin-eslint": "~5.0.0",
      "@vue/cli-service": "~5.0.0",
      "browserify": "^17.0.0",
      "eslint": "^7.32.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier-vue": "^4.2.0",
      "eslint-plugin-vue": "^8.7.1",
      "npm-run-all": "^4.1.5",
      "parcel": "^2.7.0",
      "prettier": "^2.7.1",
      "uglify-js": "^3.13.3",
      "watch": "^0.13.0",
      "webpack": "^5.53.0",
      "webpack-cli": "^4.8.0"
   },
   "dependencies": {
      "@discolabs/custard-js": "^0.1.3",
      "alpinejs": "^3.10.3",
      "core-js": "^3.8.3",
      "siema": "^1.5.1",
      "vue": "^3.2.13"
   },
   "browserslist": [
      "> 1%",
      "last 2 versions",
      "not dead",
      "not ie 11"
   ]
}
